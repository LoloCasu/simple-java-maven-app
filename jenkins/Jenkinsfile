pipeline 
{

agent {
  label 'server1'
}

tools {
  maven 'mymaven'
}

environment {
  NAME = "moninder"
}


parameters {
  string defaultValue: 'singh', name: 'LASTNAME'
}

stages{

    stage('build'){
        steps {
            sh 'mvn clean package'
            echo "hello $NAME ${params.LASTNAME}"
        }

        post {
        success {
            archiveArtifacts artifacts: '**/target/*.jar'
                    }
        }

    }

    
}

}